{{#each model as |song|}}
	<div  class='col-xs-2' style="height:200px; width:200px; display:block;overflow:hidden;">
		<span>
			{{#link-to 'song' title song.trackId }}
			
				<img  src = '{{song.artworkUrl100}}'>
				<div>
					<div>{{song.trackCensoredName}}</div>
				</div>
				
			{{/link-to}}
		</span>
	</div>

{{/each}}


